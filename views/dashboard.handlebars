                <div class="divider"></div>
            </li>
            <li>
                <a href="#!">Credits
                    <span class="badge light-green-text" data-badge-caption="">{{ points }}</span>
                </a>
            </li>
            <li>
                <a href="#!">Badges
                    <span class="new badge" data-badge-caption="Total">{{ badges.length }}</span>
                </a>
            </li>
            <li>
                <a href="#!">Messages
                    <span class="badge red-text" data-badge-caption="Unread">4</span>
                </a>
            </li>

        </ul>
        <!--End Profle Side Nav -->

        <!-- My Project Dashboard -->
        <div class="dash container">
            <h4>My Projects</h4>
            <ul class=" right tabs tabs-fixed-width">
                <li class="tab">
                    <a class="active" href="#current">Current</a>
                </li>
                <li class="tab">
                    <a href="#closed">Closed</a>
                </li>
            </ul>
            <div id="current" class="col s12">
                <ul class="collection">
                    {{#each Projects}} {{#if this.dataValues.title}} {{#unless this.dataValues.closed }}
                    <li class="collection-item avatar">
                        <a href="/projects/{{this.dataValues.id}}">
                            {{#if this.dataValues.avatarUrl}}
                            <img class="circle" src="{{ this.dataValues.avatarUrl }}"> 
                            {{/if}} 
                            {{#unless this.dataValues.avatarUrl}}
                            <i class="material-icons circle">person</i>
                            {{/unless}}
                            <h5>{{ this.dataValues.title }}</h5>
                            <a href="#!" class="secondary-content">
                                <i class="material-icons light-green-text accent-3">arrow_forward</i>
                            </a>
                        </a>
                    </li>
                    {{/unless}}{{/if}} {{/each}} 
                    {{#if Projects.[0].dataValues.title}}
                    <div style="text-align:center; margin-top:50px">
                        <a style="margin-top:25px" class="btn-large" href="/projects/post/{{ id }}">Post a Project</a>
                    </div>
                    {{/if}}
                    {{#unless Projects.[0].dataValues.title}}
                    <div style="text-align:center; margin-top:50px">
                        <div>Looks like you haven't posted any projects yet!</div>
                        <a style="margin-top:25px" class="btn-large" href="/projects/post/{{ id }}">Let's Fix That</a>
                    </div>
                    {{/unless}}
                </ul>
