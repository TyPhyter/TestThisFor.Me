                        <i class="material-icons circle">person</i>
                        {{/unless}}
                    </a>
                    <a href="#alias">
                        <span class="white-text name">{{ githubAlias }}</span>
                    </a>
                    <a href="#name">
                        <span class="white-text name">{{ githubName }}</span>
                    </a>
                    <a href="#email">
                        <span class="white-text email">{{ email }}</span>
                    </a>
                </div>
            </li>
            <li>
                <a href="#!">
                    <i class="material-icons">card_membership</i>Joined May 2018
                </a>
            </li>
